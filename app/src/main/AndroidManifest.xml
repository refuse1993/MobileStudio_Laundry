<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kim.mobilestudio_laundry">

    <application
        android:allowBackup="true"
        android:icon="@drawable/user_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AcceptedList" />
        <activity android:name=".managementActivity" />
        <activity android:name=".OrderedActivity" />
        <activity android:name=".ModifyActivity"
                  android:theme="@style/Theme.AppCompat.Light.Dialog"></activity>
    </application>

</manifest>